# .env file for Voice Typing System
# Copy this file to .env and fill in your actual values.
# This file should NOT be committed to version control.

# --- Service Selection ---
# Set to "true" to use the external service, "false" for internal.
VTS_USE_EXTERNAL_SERVICE="false"

# --- Internal Service Configuration ---
# Only needed if VTS_USE_EXTERNAL_SERVICE is "false"
VTS_INTERNAL_HOST="192.1.1.1"
VTS_INTERNAL_PORT="5000"

# --- External Service Configuration ---
# Only needed if VTS_USE_EXTERNAL_SERVICE is "true"
VTS_EXTERNAL_HOST="your-external-service.com"
VTS_EXTERNAL_USERNAME="your_username"
VTS_EXTERNAL_PASSWORD="your_password"